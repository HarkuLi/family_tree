<div class="page-header">
  <h1 class="text-center">Add Person</h1>
</div>
<div class="form-group">
  <label for="name" class="col-xs-3 control-label"><small style="color: red;">&#10036;  </small>Name</label>
  <div class="col-xs-9">
    <input class="form-control" type="text" name="name" required>
  </div>
</div>
<div class="form-group">
  <label for="birth" class="col-xs-3 control-label">Birth</label>
  <div class="col-xs-9">
    <input class="form-control" type="date" name="birth">
  </div>
</div>
<div class="form-group">
  <label for="email" class="col-xs-3 control-label">Email</label>
  <div class="col-xs-9">
    <input class="form-control" type="email" name="email">
  </div>
</div>
<div class="form-group">
  <label for="live" class="col-xs-3 control-label"><small style="color: red;">&#10036;  </small>Live</label>
  <div class="col-xs-9">
    <div class="radio-inline">
      <label>
        <input type="radio" name="live" value="true" checked>
        Live
      </label>
    </div>
    <div class="radio-inline">
      <label>
        <input type="radio" name="live" value="false">
        Deceased
      </label>
    </div>
  </div>
</div>
<div class="form-group">
  <label for="dialogEnable" class="col-xs-3 control-label"><small style="color: red;">&#10036;  </small>DialogEnable</label>
  <div class="col-xs-9">
    <div class="radio-inline">
      <label>
        <input type="radio" name="dialogEnable" value="true">
        Enable
      </label>
    </div>
    <div class="radio-inline">
      <label>
        <input type="radio" name="dialogEnable" value="false" checked>
        Disable
      </label>
    </div>
  </div>
</div>
<br>